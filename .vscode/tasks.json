{
	"version": "2.0.0",
	"tasks": [		
		{
			"label": "Build Debug STM32F7X2 Firmware",
			"type": "shell",
			"command": "make DEBUG=GDB TARGET=STM32F7X2",
			"problemMatcher": {
				"base": "$gcc",
				"fileLocation": [
					"relative",
					"${workspaceFolder}/betaflight"
				]
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"options": {
				"cwd": "${workspaceFolder}/betaflight"
			}
		}
	]
}